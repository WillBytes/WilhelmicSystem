<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="Wilhelmic_System.Views.AddEventDialog"
        Title="Add New Event" Width="400" Height="250">
	<StackPanel Margin="20">
		<TextBox x:Name="TitleBox" Watermark="Event Title" Margin="0,0,0,10"/>
		<TextBox x:Name="DescriptionBox" Watermark="Description" Margin="0,0,0,10"/>
		<TextBox x:Name="LocationBox" Watermark="Location" Margin="0,0,0,10"/>
		<Button Content="Add Event" HorizontalAlignment="Right" Click="AddEvent_Click"/>
	</StackPanel>
</Window>